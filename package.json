{
  "name": "datashell",
  "version": "0.0.2-pre-alpha",
  "description": "DatAShell command line tool",
  "main": "bundle.js",
  "type": "commonjs",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "npx http-server . -p 9966",
    "shim": "npx http-server . -p 9999",
    "budo": "budo web/page.js:bundle.js --live -- -i STATE",
    "budo:0": "#budo web/example0/page.js:bundle.js --live -- -i STATE",
    "budo:1": "budo web/example1/page.js:bundle.js --live -- -i STATE -i localdb",
    "budo:2": "budo web/example2/page.js:bundle.js --live -- -i STATE",
    "budo:3": "#budo web/example3/page.js:bundle.js --live -- -i STATE"
  },
  "bin": {
    "ds": "bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/playproject-io/datashell.git"
  },
  "keywords": ["dat", "datashell", "p2p", "hyperdht", "relay", "portal"],
  "author": "@serapath",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/playproject-io/datashell/issues"
  },
  "homepage": "https://github.com/playproject-io/datashell#readme",
  "devDependencies": {
    "browserify": "^17.0.1",
    "budo": "^11.8.4",
    "http-server": "^14.1.1"
  }
}
